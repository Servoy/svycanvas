{
  "name": "svycanvas",
  "version": "2024.3.2",
  "description": "Servoy Canvas component",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build_debug": "ng build --watch",
    "make_release": "ng build --configuration production && node scripts/build.js",
    "test": "ng test --watch --browsers Chrome",
    "test_edge": "ng test --watch --browsers Edge",
    "test_edge_nowatch": "ng test --watch=false --browsers Edge",
    "test_headless": "ng test --browsers headlessChrome --watch=false",
    "lint": "ng lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Servoy/svycanvas.git"
  },
  "keywords": [
    "servoy",
    "canvas"
  ],
  "author": "Tuan Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Servoy/svycanvas/issues"
  },
  "homepage": "https://github.com/Servoy/svycanvas#readme",
  "dependencies": {
    "@angular/core": "17.2.4",
    "@angular/forms": "17.2.4",
    "@servoy/public": "2024.3.0",
    "@types/fabric": "5.3.7",
    "fabric": "5.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.3",
    "@angular/cli": "17.2.3",
    "@angular/compiler-cli": "17.2.4",
    "adm-zip": "0.5.10",
    "eslint": "8.57.0",
    "ng-packagr": "17.2.1",
    "ts-node": "10.9.2"
  }
}